<div class="container mx-auto">
  <div class="flex max-w-5xl mx-auto">
    <div class="w-1/3">
      <%= render partial: "side_var" %>
    </div>
    <div class="w-2/3">
      <% @posts.each do |post| %>
        <%= render partial: "post_card", locals: { post: post } %>
      <% end %>
    </div>
  </div>
</div>
